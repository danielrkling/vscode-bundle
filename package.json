{
  "name": "vscode-web-rolldown",
  "displayName": "vscode-web-rolldown",
  "description": "HelloWorld example for VS Code",
  "version": "0.0.9",
  "publisher": "danielrkling",
  "repository": "https://github.com/danielrkling/vscode-bundle",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "browser": "./extension.js",
  "files":["extension.js","LICENSE"],
  "contributes": {
    "commands": [
      {
        "command": "vscodeWebRolldown.bundle",
        "title": "Bundle"
      }
    ],
    "configuration": {
      "title": "VSCode Web Rolldown Configuration",
      "properties": {
        "vscodeWebRolldown.configFilePath": {
          "type": "string",
          "default": "rolldown.config.js",
          "description": "Specify the path to your rolldown configuration file (relative to the workspace root)."
        }
      }
    }
  },
  "devDependencies": {
    "@secretlint/node": "^10.1.1",
    "@types/node": "^8.10.25",
    "@types/vscode": "^1.51.0",
    "@vscode/vsce": "^3.6.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  }
}
